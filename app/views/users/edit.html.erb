<% provide(:title, 'Edit User') %>
<h1>Update Profile</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @user do |f| %>
        <%= render 'shared/error_messages', object: f.object %>
        <%= render 'fields', f: f %>
        <%= f.submit 'Update Account', class: 'btn btn-large btn-primary' %>
    <% end %>
  </div>
</div>